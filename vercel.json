{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1",
      "continue": true,
      "headers": {
        "cache-control": "no-store"
      }
    },
    {
      "src": "/.*",
      "dest": "server/server.js"
    }
  ]
}