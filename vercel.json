{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(images|jqueryFiles|styles)/.*",
      "dest": "/client/$1"
    },
    {
      "src": "/(homepage|login).html",
      "dest": "/client/$1.html"
    },
    {
      "src": "/.*",
      "dest": "server/server.js"
    }
  ]
}